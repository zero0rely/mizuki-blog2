#!/bin/sh
basedir=$(dirname "$(echo "$0" | sed -e 's,\\,/,g')")

case `uname` in
    *CYGWIN*|*MINGW*|*MSYS*)
        if command -v cygpath > /dev/null 2>&1; then
            basedir=`cygpath -w "$basedir"`
        fi
    ;;
esac

if [ -z "$NODE_PATH" ]; then
  export NODE_PATH="/proc/cygdrive/c/Users/win10/Desktop/code/Mizuki/Mizuki-master/Mizuki-master/node_modules/.pnpm/fontmin@1.1.1/node_modules/fontmin/node_modules:/proc/cygdrive/c/Users/win10/Desktop/code/Mizuki/Mizuki-master/Mizuki-master/node_modules/.pnpm/fontmin@1.1.1/node_modules:/proc/cygdrive/c/Users/win10/Desktop/code/Mizuki/Mizuki-master/Mizuki-master/node_modules/.pnpm/node_modules"
else
  export NODE_PATH="/proc/cygdrive/c/Users/win10/Desktop/code/Mizuki/Mizuki-master/Mizuki-master/node_modules/.pnpm/fontmin@1.1.1/node_modules/fontmin/node_modules:/proc/cygdrive/c/Users/win10/Desktop/code/Mizuki/Mizuki-master/Mizuki-master/node_modules/.pnpm/fontmin@1.1.1/node_modules:/proc/cygdrive/c/Users/win10/Desktop/code/Mizuki/Mizuki-master/Mizuki-master/node_modules/.pnpm/node_modules:$NODE_PATH"
fi
if [ -x "$basedir/node" ]; then
  exec "$basedir/node"  "$basedir/../fontmin/cli.js" "$@"
else
  exec node  "$basedir/../fontmin/cli.js" "$@"
fi
